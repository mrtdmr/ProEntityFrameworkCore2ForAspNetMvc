@model long
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<td style="width:170px">
    <a class="btn btn-primary" asp-controller="Home" asp-action="Edit" asp-route-id="@Model">
        <i class="fas fa-edit"></i>
    </a>
    <a class="btn btn-success" href="@Url.Action("Details/"+Model)">
        <i class="fas fa-list-alt"></i>
    </a>
    <a class="btn btn-danger" href="@Url.Action("Delete/"+Model)">
        <i class="fas fa-trash-alt"></i>
    </a>
</td>